  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adelrodriguez/startline.git"
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "check": "biome check --write src",
    "clean": "bun scripts/clean.ts",
    "component:add": "bun scripts/add-component.ts",
    "component:diff": "shadcn diff",
    "db:check": "drizzle-kit check",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "bun scripts/seed.ts",
    "db:studio": "drizzle-kit studio",
    "deps:check": "taze -l",
    "deps:update": "taze -lw",
    "dev:email": "bun template:email && email dev --dir src/components/emails --port 3001",
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up -d",
    "format": "biome format --write src",
    "lint": "biome lint --fix --unsafe src",
    "start": "next start",
    "template:clean": "bun scripts/clean.ts",
    "template:email": "bun scripts/dev-email.ts",
    "template:schedules": "bun scripts/create-schedules.ts",
    "template:setup": "bun scripts/setup.ts",
    "template:sync": "bun scripts/sync.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit"
  },

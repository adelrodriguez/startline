{
  "name": "startline",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adelrodriguez/startline.git"
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "check": "biome check --write src",
    "clean:build": "rm -rf .next",
    "clean:data": "rm -rf .data/*",
    "clean:migrations": "rm -rf migrations/*",
    "clean": "run-p clean:*",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "deps:check": "taze -l",
    "deps:update": "taze -lw",
    "dev:email": "bun template:setup-email && email dev --dir src/components/emails --port 3001",
    "dev": "next dev",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up -d",
    "format": "biome format --write src",
    "lint": "biome lint --fix src",
    "start": "next start",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "template:setup": "bun scripts/setup.ts",
    "template:setup-email": "bun scripts/setup-dev-email.ts",
    "template:sync": "bun scripts/sync.ts",
    "ui:add": "shadcn-ui add",
    "ui:diff": "shadcn-ui diff",
    "ui:init": "shadcn-ui init"
  },
  "dependencies": {
    "@conform-to/react": "1.1.5",
    "@conform-to/zod": "1.1.5",
    "@libsql/client": "0.8.0",
    "@lucia-auth/adapter-drizzle": "1.0.7",
    "@node-rs/argon2": "1.8.3",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-separator": "1.1.0",
    "@react-email/components": "0.0.22",
    "@react-email/render": "0.0.17",
    "@sentry/nextjs": "8.21.0",
    "@t3-oss/env-core": "0.11.0",
    "@t3-oss/env-nextjs": "0.11.0",
    "@uploadthing/react": "6.7.2",
    "@upstash/ratelimit": "2.0.1",
    "@upstash/redis": "1.34.0",
    "@vercel/analytics": "1.3.1",
    "arctic": "1.9.2",
    "chalk": "5.3.0",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "drizzle-orm": "0.32.1",
    "http-status-codes": "2.3.0",
    "inngest": "3.22.1",
    "input-otp": "1.2.4",
    "lucia": "3.2.0",
    "lucide-react": "0.424.0",
    "next": "14.2.5",
    "next-intl": "3.17.2",
    "next-plausible": "^3.12.1",
    "oslo": "1.2.1",
    "posthog-js": "1.154.3",
    "posthog-node": "4.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "3.5.0",
    "server-only": "0.0.1",
    "stripe": "16.5.0",
    "tailwind-merge": "2.4.0",
    "tailwindcss-animate": "1.0.7",
    "uploadthing": "6.13.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@next/bundle-analyzer": "14.2.5",
    "@tailwindcss/typography": "0.5.13",
    "@types/bun": "1.1.6",
    "@types/node": "22.0.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "drizzle-kit": "0.23.1",
    "jiti": "1.21.6",
    "postcss": "8.4.40",
    "react-email": "2.1.6",
    "shadcn-ui": "0.8.0",
    "tailwindcss": "3.4.7",
    "taze": "0.16.3",
    "typescript": "5.5.4",
    "vitest": "2.0.5"
  },
  "type": "module"
}
